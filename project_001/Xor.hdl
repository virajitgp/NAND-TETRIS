/**
 * Exclusive-or gate:
 * if ((a and Not(b)) or (Not(a) and b)) out = 1, else out = 0
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
            Not(in=a, out=na);
            Not(in=b, out=nb);
            And(a=a, b=nb, out=o1);
            And(a=na, b=b, out=o2);
            Or(a=o1, b=o2, out=out);

}